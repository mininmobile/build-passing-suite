[tabStyle]
w = 150
h = 40
x = 150r
y = 20
stringAlign = centerCenter
stringCase = #fontCasing#
fontFace = #font#
fontSize = (14 * #scale#)
fontColor = #color#
solidColor = #background#
antiAlias = 1

[tabSelectedStyle]
fontColor = #color2#
solidColor = #background2#

[textHeaderStyle]
x = 30
y = 60
inlineSetting = underline
stringCase = #fontCasing#
stringStyle = italic
fontFace = #font#
fontSize = (32 * #scale#)
fontColor = #color2#
antiAlias = 1

[textLauncherStyle]
x = r
y = ((16 * #scale#) + (6 * #scale#))r
stringCase = #fontCasing#
fontFace = #font#
fontSize = (16 * #scale#)
fontColor = #color2#
antiAlias = 1
